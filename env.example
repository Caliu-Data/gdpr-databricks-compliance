# Databricks Configuration
DATABRICKS_HOST=https://your-workspace.cloud.databricks.com
DATABRICKS_TOKEN=your-databricks-token
DATABRICKS_CLUSTER_ID=your-cluster-id

# Database Configuration
DATABRICKS_CATALOG=gdpr_compliance
DATABRICKS_SCHEMA=governance

# Encryption Keys (generate with: openssl rand -hex 32)
PSEUDONYMIZATION_KEY=your-32-byte-hex-key-here
ENCRYPTION_KEY=your-32-byte-hex-key-here

# Retention Policy
RETENTION_YEARS=7

# Audit Configuration
AUDIT_LOG_TABLE=gdpr_compliance.governance.audit_logs
LINEAGE_TABLE=gdpr_compliance.governance.data_lineage
PII_REGISTRY_TABLE=gdpr_compliance.governance.pii_registry

# K-Anonymity Configuration
K_ANONYMITY_THRESHOLD=5

# Data Quality Thresholds
MIN_DATA_QUALITY_SCORE=0.8

